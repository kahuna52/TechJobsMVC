<h1>@ViewBag.title</h1>

<h3>@ViewBag.jobs.Count Result(s)</h3>

@{
    <table class=" job-listing">

        @foreach (Dictionary<string, string> job in ViewBag.jobs)
        {
            @foreach (KeyValuePair<string, string> listing in job)
            {
                <tbody>
                    <tr>
                        <td>@listing.Key</td>
                        <td>@listing.Value</td>
                    </tr>
                </tbody>
            }
        }
    </table>
}