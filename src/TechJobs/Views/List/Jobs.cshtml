<h1>@ViewBag.title</h1>

<h3>@ViewBag.jobs.Count Result(s)</h3>

@{
    <table >

        @foreach (Dictionary<string, string> job in ViewBag.jobs)
        {
            foreach (var listing in job)
            {
                
                    <tr >
                        <td class =" job-listing">@listing.Key</td>
                        <td class=" job-listing">@listing.Value
                    </tr>
            }
        }
    </table>
}